		if ($this->isColumnModified(SesionLogPeer::ID_SESION)) $criteria->add(SesionLogPeer::ID_SESION, $this->id_sesion);
		if ($this->isColumnModified(SesionLogPeer::FECHA)) $criteria->add(SesionLogPeer::FECHA, $this->fecha);
		if ($this->isColumnModified(SesionLogPeer::URL)) $criteria->add(SesionLogPeer::URL, $this->url);
		if ($this->isColumnModified(SesionLogPeer::MODULO)) $criteria->add(SesionLogPeer::MODULO, $this->modulo);
		if ($this->isColumnModified(SesionLogPeer::ACCION)) $criteria->add(SesionLogPeer::ACCION, $this->accion);
		if ($this->isColumnModified(SesionLogPeer::FIRMA)) $criteria->add(SesionLogPeer::FIRMA, $this->firma);
		if ($this->isColumnModified(SesionLogPeer::PUBLIC_KEY)) $criteria->add(SesionLogPeer::PUBLIC_KEY, $this->public_key);
