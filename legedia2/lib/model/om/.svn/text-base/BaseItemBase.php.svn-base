		if ($this->isColumnModified(ItemBasePeer::ID_ITEM_BASE)) $criteria->add(ItemBasePeer::ID_ITEM_BASE, $this->id_item_base);
		if ($this->isColumnModified(ItemBasePeer::ID_CAMPO)) $criteria->add(ItemBasePeer::ID_CAMPO, $this->id_campo);
		if ($this->isColumnModified(ItemBasePeer::TEXTO)) $criteria->add(ItemBasePeer::TEXTO, $this->texto);
		if ($this->isColumnModified(ItemBasePeer::NUMERO_INFERIOR)) $criteria->add(ItemBasePeer::NUMERO_INFERIOR, $this->numero_inferior);
		if ($this->isColumnModified(ItemBasePeer::NUMERO_SUPERIOR)) $criteria->add(ItemBasePeer::NUMERO_SUPERIOR, $this->numero_superior);
		if ($this->isColumnModified(ItemBasePeer::AYUDA)) $criteria->add(ItemBasePeer::AYUDA, $this->ayuda);
		if ($this->isColumnModified(ItemBasePeer::TEXTO_AUXILIAR)) $criteria->add(ItemBasePeer::TEXTO_AUXILIAR, $this->texto_auxiliar);
		if ($this->isColumnModified(ItemBasePeer::ORDEN)) $criteria->add(ItemBasePeer::ORDEN, $this->orden);
		if ($this->isColumnModified(ItemBasePeer::ES_RESPONSABLE_FICHERO)) $criteria->add(ItemBasePeer::ES_RESPONSABLE_FICHERO, $this->es_responsable_fichero);
		if ($this->isColumnModified(ItemBasePeer::ES_INCONSISTENTE)) $criteria->add(ItemBasePeer::ES_INCONSISTENTE, $this->es_inconsistente);
		if ($this->isColumnModified(ItemBasePeer::BORRADO)) $criteria->add(ItemBasePeer::BORRADO, $this->borrado);

		return $criteria;
